{
  "name": "express-react-svelte-webcomponent",
  "version": "0.1.0",
  "private": false,
  "description": "Webpack multi-compiler example to bundle builds for React, Svelte, Web Components, and normal Express view templates.",
  "engines": {
    "node": "12.x"
  },
  "main": "dist/server.js",
  "scripts": {
    "lint": "$npm_execpath eslint './src/**/*.{js,jsx}'",
    "test": "$npm_execpath jest",
    "clean": "rm -fR dist/public rm -fR dist/views",
    "build": "yarn clean && NODE_ENV=production parallel-webpack --config webpack.config.js",
    "build:dev": "yarn clean && NODE_ENV=development parallel-webpack --config webpack.config.js",
    "profile": "node --inspect-brk node_modules/webpack/bin/webpack.js --config webpack.config.js",
    "analyze": "yarn clean && ANALYZE=true yarn build:dev",
    "start": "NODE_ENV=development node dist/server.js"
  },
  "repository": "https://github.com/minagawah/express-react-svelte-webcomponent.git",
  "homepage": "",
  "keywords": [
    "multi-compiler",
    "React",
    "Svelte",
    "Express",
    "Web Components"
  ],
  "author": "Hiroki Minagawa <minagawa@tokyo800.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minagawah/express-react-svelte-webcomponent/issues"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "animejs": "^3.1.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "nunjucks": "^3.2.0",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "svelte": "^3.16.7",
    "tailwind.macro": "^1.0.0-alpha.10"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.8.0",
    "copy-webpack-plugin": "^5.1.1",
    "csp-html-webpack-plugin": "^3.0.4",
    "css-loader": "^3.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.27",
    "license-webpack-plugin": "^2.1.3",
    "mini-css-extract-plugin": "^0.9.0",
    "nunjucks-loader": "^3.0.0",
    "parallel-webpack": "^2.4.0",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.1.2",
    "svelte-loader": "^2.13.6",
    "tailwindcss": "^1.1.4",
    "to-string-loader": "^1.1.6",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
